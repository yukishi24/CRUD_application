<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.Employee">
	<resultMap type="com.example.demo.mybatis.entity.Employee"
		id="Employee">
	<result column="id" property="employee_id"/>
	<result column="name" property="employee_name"/>
	<result column="admin" property="admin"/>
	</resultMap>
	<select id="findAll" resultMap="Employee">
	select id, name, admin from employee
	</select>
</mapper>