<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.EmployeeMapper">
	<resultMap type="com.example.demo.mybatis.entity.Employee"
		id="employeeMap">
		<result column="id" jdbcType="INTEGER" property="employee_id" />
		<result column="name" jdbcType="VARCHAR"
			property="employee_name" />
		<result column="admin" jdbcType="VARCHAR" property="employee_admin" />
	</resultMap>
	<select id="selectAll" resultMap="employeeMap">
		select id, name, admin from employee
	</select>
</mapper>